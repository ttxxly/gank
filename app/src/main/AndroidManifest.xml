<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ttxxly.top.gank">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".modules.launcher.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.home.HomeActivity" />
        <activity android:name=".modules.web.WebActivity" />
        <activity android:name=".modules.daily.DailyGankActivity" />
        <activity android:name=".modules.girl.GirlActivity" />
        <activity android:name=".modules.search.SearchActivity" />
        <activity android:name=".modules.about.AboutActivity" />
        <activity android:name=".modules.category.CategoryActivity"></activity>
    </application>

</manifest>